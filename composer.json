{
    "name": "dpazuic/aits_student_enrollment",
    "description": "Package used to query AITS webservices for a UIN's enrollment data",
    "type": "library",
    "require": {
        "vlucas/phpdotenv": "^3.3",
        "nesbot/carbon": "^1.36",
        "guzzlehttp/guzzle": "^6.3",
        "monolog/monolog": "^1.24",
        "php": ">5.6.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "dpazuic",
            "email": "dpaz@uic.edu"
        }
    ],
    "autoload": {
        "classmap": [
            "app"
        ],
        "files": [
            "app/bootstrap.php"
        ]
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.6",
        "phpcompatibility/php-compatibility": "^9.3"
    },
    "scripts": {
        "sniffer:verbose:php74": "phpcs -v --extensions=php --ignore=*/vendor/* -p . --report-file=aits_student_enrollment_php74-verbose-migration-report-$(date +%Y%m%d-%H%M%S).txt --standard=vendor/phpcompatibility/php-compatibility/PHPCompatibility --runtime-set testVersion 7.4",
        "sniffer:php74": "phpcs --extensions=php --ignore=*/vendor/* -p . --report-file=aits_student_enrollment_php74-migration-report-$(date +%Y%m%d-%H%M%S).txt --standard=vendor/phpcompatibility/php-compatibility/PHPCompatibility --runtime-set testVersion 7.4"
    }
}
